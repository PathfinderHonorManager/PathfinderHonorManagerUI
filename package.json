{
  "name": "pathfinderhonormanagerui",
  "version": "0.5.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@1.22.21",
  "scripts": {
    "dev": "vite dev --port 3000",
    "dev:local": "VITE_USE_LOCAL_API=true vite dev --port 3000",
    "dev:prod": "VITE_USE_LOCAL_API=false vite dev --port 3000",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview --port 5050",
    "test": "vitest --environment jsdom --run",
    "test:unit": "vitest --environment jsdom --run --exclude \"**/*.integration.spec.ts\"",
    "test:integration": "vitest --config vitest.integration.config.ts --run",
    "test:unit:watch": "vitest --environment jsdom",
    "test:stores": "vitest --environment jsdom --run src/stores/**/*.spec.ts",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --config eslint.config.js --format compact --fix",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@auth0/auth0-vue": "^2.5.0",
    "@fortawesome/fontawesome-svg-core": "^7.1.0",
    "@fortawesome/free-solid-svg-icons": "^7.1.0",
    "@fortawesome/vue-fontawesome": "^3.1.3",
    "@microsoft/applicationinsights-web": "^3.3.11",
    "@vue/tsconfig": "^0.8.1",
    "axios": "^1.13.2",
    "fontawesome": "^5.6.3",
    "jwt-decode": "^4.0.0",
    "pinia": "3.0.4",
    "pinia-plugin-persistedstate": "^4.7.1",
    "prettier": "^3.8.0",
    "vue": "^3.3.4",
    "vue-router": "^4.6.4",
    "vue3-simple-typeahead": "^1.0.11"
  },
  "devDependencies": {
    "@pinia/testing": "^1.0.3",
    "@rushstack/eslint-patch": "^1.15.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.2",
    "@semantic-release/npm": "^13.1.3",
    "@semantic-release/release-notes-generator": "^14.1.0",
    "@types/jsdom": "^27.0.0",
    "@types/lodash": "^4.17.23",
    "@types/node": "^25.0.9",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitest/coverage-v8": "^4.0.17",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.2",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-formatter-compact": "^9.0.1",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-vue": "^10.6.2",
    "flush-promises": "^1.0.2",
    "jsdom": "^27.4.0",
    "msw": "^2.12.7",
    "semantic-release": "^25.0.2",
    "typescript": "~5.9.3",
    "vite": "^7.3.0",
    "vitest": "^4.0.17",
    "vue-tsc": "^3.2.2"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
